        src: url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.eot?#iefix') format('embedded-opentype'), url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.ttf') format('truetype'), url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.woff') format('woff'), url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.woff2') format('woff2');
