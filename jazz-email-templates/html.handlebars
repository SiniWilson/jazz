        font-family: 'Lato', arial !important;
      }

    </style>
    <!--<![endif]-->

</head>
<body style="margin: 0; mso-line-height-rule: exactly;">
  <div class="wrapper clearfix">
    <div class="container">
      <header class="mr-header">
        <article class="clearfix">
          <div class="clearfix logo-wrp"><!-- <img class="logo" src="tmobile.png"> -->
          <img class="logo" src="https://s3-us-west-2.amazonaws.com/asgc-email-templates/welcome/assets/jazz-logo.png">
          </div>
        </article>
      </header>
      <article class="contents clearfix">
        <div class="approve common">
          <div style="max-height:0;width:100%;overflow: visible;">
            <img class="dottedimg" height="60px" src="https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Pattern-Header-01%401x.png.png" width="60%">
          </div>
          {{heading}}
        </div>
        <div class="user common">
          Hello {{name.first}},
        </div>
        <div class="message common">
          {{{message}}}
        </div>
        {{#if details}}
        <div class="details common">{{{details}}}</div>
        {{/if}}
        {{#if linkexpire}}
        <div class="linkexpire common">
          {{linkexpire}}
        </div>
        {{/if}}
        {{#if link.length}}
        <div style=" margin:auto; display: block; text-align: center;" class="button-wrp">
          {{#each link}} <a class="buttonclick {{text}}" href="{{url}}">{{text}}</a> {{/each}}
        </div>
        {{/if}}
      </article>
    </div>
    <footer class="mr-footer">
      <div class="footer-link-wrapper">
        <a class="footer-link" href="http://jazz.corporate.t-mobile.com" target="_blank">Jazz</a> <a class="footer-link" href="https://tmobile.atlassian.net/wiki/spaces/CAPI/pages/218171156/Getting+Started+with+Service+Development+using+Jazz" target="_blank">Docs</a>
      </div>
    </footer>
  </div>
</body>
</html>
