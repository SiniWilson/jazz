    font-family: 'Lato';
    font-style: normal;
    font-weight: 400;
    src: url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.eot?#iefix') format('eot'),url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.ttf') format('ttf'), url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.woff') format('woff'), url('https://s3-us-west-2.amazonaws.com/asgc-email-templates/reset/assets/Lato-Regular.woff2') format('woff2');
