<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="cf_ExtractPathParams">
    <DisplayName>cf_ExtractPathParams</DisplayName>
    <Add>
        <Headers>
            <Header name="CompletePath">{proxy.basepath}{proxy.pathsuffix}</Header>
        </Headers>
    </Add>
    <Add>
        <Headers>
            <Header name="PathParam">{proxy.pathsuffix}</Header>
        </Headers>
    </Add>
    <Add>
        <Headers>
            <Header name="ProxyName">{apiproxy.name}</Header>
        </Headers>
    </Add>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>