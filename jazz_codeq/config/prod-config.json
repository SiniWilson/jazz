{
	"CAS_SONAR_SECRETS_LOCATION": "{conf-cas-cdp-secret}",
	"sonar_url": "https://sonar.cdp.t-mobile.com/api/measures/search_history",
	"sonar_search_metrics_url": "https://sonar.cdp.t-mobile.com/api/metrics/search?f=name",
	"sonar_project_key": "jazz",
	"allowed_metrics": ["security", "code-coverage", "code-smells", "lines-of-code", "files", "vulnerabilities"],
	"metric_map": {
		"security": "vulnerabilities",
		"code-coverage": "coverage",
		"code-smells": "code_smells",
		"lines-of-code": "ncloc",
		"files": "files",
		"vulnerabilities": "high_severity_vulns"
	},
	"gitlabTokenPath": "{conf-cas-gitlab-token-location}",
	"gitlabUrl": "https://gitlab.com/api/v4/projects",
	"required_params": ["physicalId", "serviceRepoUrl"],
	"SERVICE_ID_HEADER_KEY": "Jazz-Service-ID",
	"help_service": "https://{conf-host-name}/api/jazz/codeq/help",
	"metrics_help": {
		"security": {
			"description": "Number of security vulnerabilities",
			"unit": "count",
			"minValue": "0",
			"maxValue": "NA"
		},
		"code-coverage": {
			"description": "Coverage answers the following question: 'Has each boolean expression been evaluated both to true and false?'. This is the density of possible branches in flow control structures that have been followed during unit tests execution.",
			"unit": "percentage",
			"minValue": "0",
			"maxValue": "100"
		},
		"code-smells": {
			"description": "Certain structures in the code that indicate violation of fundamental design principles and negatively impact design quality",
			"unit": "count",
			"minValue": "0",
			"maxValue": "NA"
		},
		"lines-of-code": {
			"description": "Number of lines of code.",
			"unit": "count",
			"minValue": "0",
			"maxValue": "NA"
		},
		"files": {
			"description": "Number of files.",
			"unit": "count",
			"minValue": "0",
			"maxValue": "NA"
		},
		"vulnerabilities": {
			"description": "Number of high-severity security vulnerabilities",
			"unit": "count",
			"minValue": "0",
			"maxValue": "NA"
		}
	}
}
